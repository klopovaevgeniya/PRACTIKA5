<Window x:Class="PRACTIKA5.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRACTIKA5"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid Background="PapayaWhip">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="7*"/>
            <RowDefinition Height="55*"/>
        </Grid.RowDefinitions>

        <Button Background="BurlyWood" Content="Назад" Grid.Column="0" Grid.Row="0" Click="Button_Click_3" />
        <Button Background="BurlyWood" Content="Сотрудники" Grid.Column="1" Grid.Row="0" Click="Button_Click"/>
        <Button Background="BurlyWood" Content="Специальность" Grid.Column="2" Grid.Row="0" Click="Button_Click_1"/>
        <Button Background="BurlyWood" Content="Поставщики" Grid.Column="3" Grid.Row="0" Click="Button_Click_2"/>
        <Frame x:Name="Frame" Grid.ColumnSpan="4" Grid.Row="1"/>
    </Grid>
</Window>
